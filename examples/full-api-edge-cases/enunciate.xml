<enunciate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://enunciate.webcohesion.com/schemas/enunciate-2.0.0-M.4.xsd">

  <api-classes>
    <exclude pattern="org.glassfish.**"/>
    <exclude pattern="com.webcohesion.enunciate.examples.jaxwsrijersey.genealogy.other.*"/>
  </api-classes>

  <modules>
    <csharp-xml-client disableCompile="false"/>
    <jaxrs groupBy="path"/>
    <java-xml-client>
      <package-conversions>
        <convert from="com.webcohesion.enunciate.examples.jaxwsrijersey.genealogy" to="genealogy.client"/>
      </package-conversions>
    </java-xml-client>
  </modules>

</enunciate>